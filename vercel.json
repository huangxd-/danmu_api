{
  "version": 2,
  "builds": [
    {
      "src": "danmu_api/worker.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/danmu_api/worker.js"
    },
    {
      "src": "/([^/]+\\.html)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/index.html"
    }
  ]
}
